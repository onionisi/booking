              content: '<img id="picbig" style="width: 300px;" src="http://m.kuaishubao.com/m/g/'+$("#picbtn").attr("bigsrc")+'">'
          });
          if ($.support.touch){
              $("#picbtn").tap(function(){dialog1.open()});
              $("#picbig").tap(function(){dialog1.close()});
              $("#twxqbtn").tap(function(){
                  twxq_show();
                  return false;
              });
              $("#addcart").tap(function(){
                  add_cart('A230028899621', $('#buy_num').val());
                  try{
                      show_cartnum();
                  }catch (e){}
              });
              $("#buynow").tap(function(){
                  add_cart('A230028899621', $('#buy_num').val(), 1, true);
              });
          }else{
              $("#picbtn").click(function(){dialog1.open()});
              $("#picbig").click(function(){dialog1.close()});
              $("#twxqbtn").click(function(){
                  twxq_show();
                  return false;
              });
