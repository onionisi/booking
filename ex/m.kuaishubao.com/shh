    <script>
        $("img[data-url]").imglazyload();
        cctiy = {
            'bj':'北京市',
            'shh':'上海市',
            'chd':'成都市',
            'chsh':'长沙市',
            'xian':'西安市',
            'hzh':'杭州市',
            'shz':'深圳市'
        };
        if ($.cookie("auto_city") == '' || $.cookie("auto_city") == null ){
            $(window).location(function(rs){
                        //console.log(rs)
                        //alert(rs.address)
                        for(i in cctiy){
                            if(rs.addressComponents.city == cctiy[i] && i != $.cookie("cookie_shop_id")){
                                if(confirm("是否切换到"+cctiy[i])){
                                    location.href = "/"+i;
                                }
                            }else{
                                $.cookie("auto_city", '1');
                            }
                        }
                    },
                    function(){},
                    {timeout:10000, maximumAge:60}
            );
        }
    </script>
